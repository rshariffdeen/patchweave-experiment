BUGGY_FILE=examples/jpegdec.c
BINARY_PATH=examples/cwebp
EXPLOIT="/cwebp  \$POC -o out.webp"
POC_PATH=/tests/cross-program/openjpeg-libwebp/signed-int-overflow/1.jp2
FUZZ_DIR_IN=fuzz_in
FUZZ_DIR_OUT=fuzz_out


mkdir {pa,pb}
git reset --hard HEAD
autoreconf -i
CC=clang ./configure CFLAGS='-g -O0 -fsanitize=integer -static'
make clean
make clean all
make -j32
cp $BINARY_PATH pa
mv examples/f1x-*.patch f1x.patch
patch $BUGGY_FILE < f1x.patch
make -j32
cp $BINARY_PATH pb
touch diff_fuzz.log
mkdir {$FUZZ_DIR_IN,$FUZZ_DIR_OUT}
python3 /experiment/patchweave/scripts/diff_fuzz.py $POC_PATH $FUZZ_DIR_IN $FUZZ_DIR_OUT pa pb $EXPLOIT